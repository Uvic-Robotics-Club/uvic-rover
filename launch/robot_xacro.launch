<?xml version="1.0"?>
<launch>

    <include file="/opt/ros/noetic/share/gazebo_ros/launch/empty_world.launch">
        <arg name="paused" value="false" />
        <arg name="use_sim_time" value="true" />
        <arg name="gui" value="true" />
        <arg name="headless" value="false" />
        <arg name="debug" value="false" />
    </include>

        <param name="rover_description" command="$(find xacro)/xacro $(find uvic_rover)/rover_description/UVicArm.xacro" />

<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model uvicarm -param rover_description" />

</launch>